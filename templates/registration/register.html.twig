{% extends 'base.html.twig' %}

{% block title %}Inscription{% endblock %}

{% block body %}
	<div class="col-sm-10 col-sm offset-1" style="padding-top:5%">
		<h4>Inscription</h4>
		<br>
		{{ form_start(form) }}
		<br>
		{{ form_row(form.firstname) }}
		<br>
		{{ form_row(form.lastname) }}
		<br>
		{{ form_row(form.email) }}
		<br>
		{{ form_row(form.address) }}
		<br>
		{{ form_row(form.phone) }}
		<br>
		{{ form_row(form.password) }}
		<br>
		{{ form_row(form.newsletter) }}
		<br>
		{{ form_row(form.submit) }}
		<br>
		{{ form_row(form.slug) }}	
		{{ form_end(form) }}
	</div>
{% endblock %}
