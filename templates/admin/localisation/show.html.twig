{% extends 'base.html.twig' %}

{% block title %}Localisation
{% endblock %}

{% block body %}
	<h2>Détail Localisation</h2>
	<div class="table-responsive">
		<table class="table table-bordered table-sm" id="dataTable" width="100%" cellspacing="0">
			<tbody>
				<tr></tr>
				<tr>
					<th>Nom</th>
					<td>{{ localisation.name }}</td>
				</tr>
			</tbody>
		</table>
		<a class="btn btn-outline-primary btn-sm" href="{{ path('localisation_edit', {'id': localisation.id}) }}" role="button">Edition</a>
		{{ include('/admin/localisation/_delete_form.html.twig') }}
		<a class="btn btn-outline-secondary btn-sm" href="{{ path('localisation_list') }}" role="button">Retour à la Liste</a>

	{% endblock %}
