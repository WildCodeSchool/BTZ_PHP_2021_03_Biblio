{% extends 'admin/layout.html.twig' %}

{% block title %}Localisation
{% endblock %}

{% block content %}
<section class="py-4">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12 col-md-6">
				<h2>Détail Localisation</h2>
					<table class="table table-bordered table-sm" id="dataTable" width="100%" cellspacing="0">
						<tbody>
							<tr></tr>
							<tr>
								<th>Nom</th>
								<td>{{ localisation.name }}</td>
							</tr>
						</tbody>
					</table>
					<a class="btn btn-outline-primary btn-sm mb-2" href="{{ path('localisation_edit', {'id': localisation.id}) }}" role="button">Edition</a>
					{{ include('/admin/localisation/_delete_form.html.twig') }}
					<a class="btn btn-outline-secondary btn-sm mt-2" href="{{ path('localisation_list') }}" role="button">Retour à la Liste</a>
			
            </div>

    </div>
</section>
	{% endblock %}
