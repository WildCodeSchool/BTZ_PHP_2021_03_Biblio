{% extends 'admin/layout.html.twig' %}

{% block title %}Mot clé Géolocalisation {% endblock %}

{% block content %}
<section class="py-4">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12 col-md-6">
                <h1>Mot clé {{ keyword_geo.name }} </h1>
                <div class="table-responsive">
                    <table class="table table-bordered table-sm" id="dataTable" width="100%" cellspacing="0">
                        <tbody>
                            <tr>
                                <th>Nom</th>
                                <td>{{ keyword_geo.name }}</td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <a class="btn btn-outline-primary btn-sm mb-2" href="{{ path('keyword_geo_edit', {'id': keyword_geo.id}) }}" role="button">Editer</a>
                {{ include('/admin/keyword_geo/_delete_form.html.twig') }}
                <a class="btn btn-outline-secondary btn-sm mt-2" href="{{ path('keyword_geo_list') }}">Retour</a>
            </div>
        </div>
    </div>
</section>  
{% endblock %}
